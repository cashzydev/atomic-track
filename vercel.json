{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "atomictrack.com.br"
        }
      ],
      "destination": "https://www.atomictrack.com.br/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/auth/callback(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        }
      ]
    }
  ]
}